{% extends 'SkonsoftLookForTravelerBundle::layout.html.twig' %}
{% block content %}

<h1 class="page-header">{{ 'User'|trans }}</h1>

<table class="record_properties table table-bordered table-condensed table-striped">
    <tbody>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Usernamecanonical</th>
            <td>{{ entity.usernameCanonical }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Emailcanonical</th>
            <td>{{ entity.emailCanonical }}</td>
        </tr>
        <tr>
            <th>Enabled</th>
            <td><i class="{{ entity.enabled ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Salt</th>
            <td>{{ entity.salt }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Lastlogin</th>
            <td>{% if entity.lastLogin %}{{ entity.lastLogin|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Locked</th>
            <td><i class="{{ entity.locked ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Expired</th>
            <td><i class="{{ entity.expired ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Expiresat</th>
            <td>{% if entity.expiresAt %}{{ entity.expiresAt|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Confirmationtoken</th>
            <td>{{ entity.confirmationToken }}</td>
        </tr>
        <tr>
            <th>Passwordrequestedat</th>
            <td>{% if entity.passwordRequestedAt %}{{ entity.passwordRequestedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Roles</th>
            <td>{{ entity.roles }}</td>
        </tr>
        <tr>
            <th>Credentialsexpired</th>
            <td><i class="{{ entity.credentialsExpired ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Credentialsexpireat</th>
            <td>{% if entity.credentialsExpireAt %}{{ entity.credentialsExpireAt|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Updatedat</th>
            <td>{% if entity.updatedAt %}{{ entity.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Dateofbirth</th>
            <td>{% if entity.dateOfBirth %}{{ entity.dateOfBirth|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Firstname</th>
            <td>{{ entity.firstname }}</td>
        </tr>
        <tr>
            <th>Lastname</th>
            <td>{{ entity.lastname }}</td>
        </tr>
        <tr>
            <th>Website</th>
            <td>{{ entity.website }}</td>
        </tr>
        <tr>
            <th>Biography</th>
            <td>{{ entity.biography }}</td>
        </tr>
        <tr>
            <th>Gender</th>
            <td>{{ entity.gender }}</td>
        </tr>
        <tr>
            <th>Locale</th>
            <td>{{ entity.locale }}</td>
        </tr>
        <tr>
            <th>Timezone</th>
            <td>{{ entity.timezone }}</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>{{ entity.phone }}</td>
        </tr>
        <tr>
            <th>Facebookuid</th>
            <td>{{ entity.facebookUid }}</td>
        </tr>
        <tr>
            <th>Facebookname</th>
            <td>{{ entity.facebookName }}</td>
        </tr>
        <tr>
            <th>Facebookdata</th>
            <td>{{ entity.facebookData }}</td>
        </tr>
        <tr>
            <th>Twitteruid</th>
            <td>{{ entity.twitterUid }}</td>
        </tr>
        <tr>
            <th>Twittername</th>
            <td>{{ entity.twitterName }}</td>
        </tr>
        <tr>
            <th>Twitterdata</th>
            <td>{{ entity.twitterData }}</td>
        </tr>
        <tr>
            <th>Gplusuid</th>
            <td>{{ entity.gplusUid }}</td>
        </tr>
        <tr>
            <th>Gplusname</th>
            <td>{{ entity.gplusName }}</td>
        </tr>
        <tr>
            <th>Gplusdata</th>
            <td>{{ entity.gplusData }}</td>
        </tr>
        <tr>
            <th>Token</th>
            <td>{{ entity.token }}</td>
        </tr>
        <tr>
            <th>Twostepverificationcode</th>
            <td>{{ entity.twoStepVerificationCode }}</td>
        </tr>
        <tr>
            <th>Maritalstatus</th>
            <td><i class="{{ entity.maritalStatus ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Kidsnumber</th>
            <td><i class="{{ entity.kidsNumber ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Nationality</th>
            <td>{{ entity.nationality }}</td>
        </tr>
        <tr>
            <th>Taille</th>
            <td>{{ entity.taille }}</td>
        </tr>
        <tr>
            <th>Silouhette</th>
            <td><i class="{{ entity.silouhette ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Hairclor</th>
            <td><i class="{{ entity.hairClor ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Eyecolor</th>
            <td><i class="{{ entity.eyeColor ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Residencecountry</th>
            <td>{{ entity.residenceCountry }}</td>
        </tr>
        <tr>
            <th>Residencetown</th>
            <td>{{ entity.residenceTown }}</td>
        </tr>
        <tr>
            <th>Studylevel</th>
            <td><i class="{{ entity.studyLevel ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Activitysector</th>
            <td><i class="{{ entity.activitySector ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Profession</th>
            <td><i class="{{ entity.profession ? 'icon-check' : '' }}"></i></td>
        </tr>
        <tr>
            <th>Careerdescription</th>
            <td>{{ entity.careerDescription }}</td>
        </tr>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
    </tbody>
</table>

    <p>
        <a class="btn" href="{{ path('user_edit', { 'id': entity.id }) }}">
            <i class="icon-pencil"></i>
            {{ 'edit'|trans }}
        </a>
    </p>

    <form action="{{ path('user_delete', { 'id': entity.id }) }}" method="post">
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit">
            <i class="icon-trash"></i>
            {{ 'delete'|trans }}
        </button>
    </form>

    <a class="btn" href="{{ path('user') }}">
        <i class="icon-th-list"></i>
        {{ 'back to the list'|trans }}
    </a>

{% endblock %}